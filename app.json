{
  "expo": {
    "name": "React Native Indecision",
    "slug": "react-native-indecision",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/indecision-logo184trans.png",
    "scheme": "Indecision",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/indecision-logo184trans.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Indecision"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "Indecision"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
